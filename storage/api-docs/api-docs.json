        "/api/attempts/answer": {
            "post": {
                "tags": [
                    "Quiz Attempts"
                ],
                "summary": "Submit an answer for a question",
                "operationId": "d8f994d4fc2f275c565a0a1c270218f5",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "attempt_id",
                                    "question_id",
                                    "selected_option_id"
                                ],
                                "properties": {
                                    "attempt_id": {
                                        "type": "integer",
                                        "example": 3
                                    },
                                    "question_id": {
                                        "type": "integer",
                                        "example": 10
                                    },
                                    "selected_option_id": {
                                        "type": "integer",
                                        "example": 42
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Answer stored",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "your_answer": {
                                            "type": "string",
                                            "example": "Laravel is a PHP framework"
                                        },
                                        "is_correct": {
                                            "type": "boolean",
                                            "example": true
                                        },
                                        "correct_answer": {
                                            "type": "string",
                                            "example": "Laravel is a PHP framework"
                                        },
                                        "correct_option_id": {
                                            "type": "integer",
                                            "example": 42
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/quiz/finish": {
            "post": {
                "tags": [
                    "Quiz Attempts"
                ],
                "summary": "Finish quiz attempt and calculate results",
                "operationId": "88090d4a751820efb1b2c78201c388df",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
